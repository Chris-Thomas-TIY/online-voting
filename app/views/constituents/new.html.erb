<h1>Constituents#new</h1>
<p>Find me in app/views/constituents/new.html.erb</p>

<%= form_for @constituent do |f| %>
  <div class="constituent">
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Save" %>
  </div>
<% end %>
