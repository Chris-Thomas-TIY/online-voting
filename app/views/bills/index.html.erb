<h1>Bills#index</h1>
<p>Find me in app/views/bills/index.html.erb</p>
<%= link_to 'Create Bill', new_bill_path %>

<table>
  <thead>
    <h1> Bills</h1>
  </thead>

  <tbody>
    <% @bills.each do |b| %>
      <tr>
        <td> <%= b.name %> </td>
        <td> <%= b.actual_vote %> </td>
      </tr>
    <%end%>
  </tbody>
</table>
